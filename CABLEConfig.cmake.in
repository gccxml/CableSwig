# The full path to the main cable executable.
SET(CABLE "@CABLE_EXECUTABLE_FILE@")

# The full path to the CableTclFacility library.
SET(CABLE_TCL_LIBRARY "@CABLE_TCL_LIBRARY@")

# The CABLE include file directory.
SET(CABLE_INCLUDE_DIRS "@CABLE_INCLUDE_DIRS@")

# The CABLE library directories.
SET(CABLE_LIBRARY_DIRS "@CABLE_LIBRARY_DIRS@")

# The CABLE version number
SET(CABLE_VERSION_MAJOR "@CABLE_VERSION_MAJOR@")
SET(CABLE_VERSION_MINOR "@CABLE_VERSION_MINOR@")
SET(CABLE_VERSION_PATCH "@CABLE_VERSION_PATCH@")

# The C++ compiler used to build CABLE.
SET(CABLE_CXX_COMPILER "@CMAKE_CXX_COMPILER@")

# The C++ compiler flags used to build CABLE.
SET(CABLE_CXX_FLAGS "@CMAKE_CXX_FLAGS@")

# Check if the compiler matches the one used to build CABLE.
IF(CMAKE_CXX_COMPILER STRLESS CABLE_CXX_COMPILER)
  SET(CABLE_CXX_COMPILER_MISMATCH 1)
ELSE(CMAKE_CXX_COMPILER STRLESS CABLE_CXX_COMPILER)
  IF(CMAKE_CXX_COMPILER STRGREATER CABLE_CXX_COMPILER)
    SET(CABLE_CXX_COMPILER_MISMATCH 1)
  ENDIF(CMAKE_CXX_COMPILER STRGREATER CABLE_CXX_COMPILER)
ENDIF(CMAKE_CXX_COMPILER STRLESS CABLE_CXX_COMPILER)

# Check if the compiler flags match those used to build CABLE.
IF(CMAKE_CXX_FLAGS STRLESS CABLE_CXX_FLAGS)
  SET(CABLE_CXX_FLAGS_MISMATCH 1)
ELSE(CMAKE_CXX_FLAGS STRLESS CABLE_CXX_FLAGS)
  IF(CMAKE_CXX_FLAGS STRGREATER CABLE_CXX_FLAGS)
    SET(CABLE_CXX_FLAGS_MISMATCH 1)
  ENDIF(CMAKE_CXX_FLAGS STRGREATER CABLE_CXX_FLAGS)
ENDIF(CMAKE_CXX_FLAGS STRLESS CABLE_CXX_FLAGS)
