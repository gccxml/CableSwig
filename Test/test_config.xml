<?xml version="1.0"?>
<Package name="Vect3">
  <Dependencies>
    <Package name="Numbers"/>
  </Dependencies>

  <Headers>
    <Directory name="Code/Common"/>
    <File name="itkVersion.h"/>
  </Headers>

  <Set name="Dimension">
    <Element tag="_1">1</Element>
    <Element tag="_2">2</Element>
    <Element tag="_3">3</Element>
  </Set>

  <Set name="signedness">
    <Element tag="_"></Element>
    <Element tag="_U">unsigned </Element>
  </Set>

  <Set name="scalarType">
    <Element tag="char">char</Element>
    <Element tag="short">short</Element>
    <Element tag="int">int</Element>
    <Element tag="long">long</Element>
    <Element tag="llong">long long</Element>
  </Set>

  <Set name="BasicScalarType">
    <Element tag="">$signedness$scalarType</Element>
  </Set>

  <Namespace name="itk" prefix_seperator="">
    <Code name="New()">
      <![CDATA[ 
               _wrap_TypeName::Pointer newObject = _wrap_TypeName::New();
               newObject->Register();
               return newObject.GetPointer();
              ]]>
    </Code>
    
    <Code name="Delete()">
      <![CDATA[
               static_cast<_wrap_TypeName*>(object)->Delete();
              ]]>
    </Code>
    <Set name="ImageType">
      <Element tag="Image"><![CDATA[Image<$BasicScalarType, $Dimension>]]></Element>
    </Set>
  </Namespace>

  <WrapperSet>
    <!--
    <Method purpose="create" code="New()"/>
    <Method purpose="create" code="Delete()"/>
    -->
    <Set name="::itk::ImageType"/>
    <Element tag="Directory">Directory</Element>
    <Element tag="Pair"><![CDATA[pair<$itk::ImageType, $Dimension>]]></Element>
  </WrapperSet>

</Package>
