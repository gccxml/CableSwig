<?xml version="1.0"?>
<Package name="Vect3">
  <Dependencies>
    <Package name="Numbers"/>
  </Dependencies>

  <Headers>
    <Directory name="Code/Common"/>
    <File name="itkVersion.h"/>
  </Headers>

  <ArgumentSet name="Dimension">
    <Argument tag="1">1</Argument>
    <Argument tag="2">2</Argument>
    <Argument tag="3">3</Argument>
  </ArgumentSet>

  <ArgumentSet name="Integer">
    <Argument tag="I">int</Argument>
  </ArgumentSet>

  <ArgumentSet name="BasicScalarType">
    <Argument tag="F">float</Argument>
    <ArgumentSet name="Integer"/>
  </ArgumentSet>

  <CreateMethod name="operator new">
    <![CDATA[
             return new _wrap_TypeName;
             ]]>
  </CreateMethod>

  <ArgumentSet name="VectorOfScalar">
    <Argument tag="Vector"><![CDATA[vector<$BasicScalarType, $Dimension>]]></Argument>
  </ArgumentSet>

</Package>
